   "source": [
    "Write a function to calculate the factorial of a number."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Factorial of 0 is 1\n"
     ]
    }
   ],
   "source": [
    "def factorial(num):\n",
    "    fact = 1\n",
    "    while num >1:\n",
    "        fact = num* fact\n",
    "        num -= 1\n",
    "        \n",
    "    return fact\n",
    "\n",
    "num = int(input())\n",
    "print(f\"Factorial of {num} is {factorial(num)}\")\n",
    "\n"
   ]
  },
